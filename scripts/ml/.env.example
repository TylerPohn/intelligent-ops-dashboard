# AWS Configuration for ML Training Pipeline
# Copy this file to .env and fill in your values

# AWS Region
AWS_REGION=us-east-1

# DynamoDB Configuration
DYNAMODB_TABLE=IOPSInsights

# S3 Configuration
S3_BUCKET=iops-ml-training

# SageMaker Configuration
SAGEMAKER_EXECUTION_ROLE=arn:aws:iam::YOUR_ACCOUNT_ID:role/SageMakerExecutionRole

# Training Configuration
INSTANCE_TYPE=ml.m5.xlarge
ENDPOINT_INSTANCE_TYPE=ml.t3.medium
MIN_INSTANCES=1
MAX_INSTANCES=3

# Tuning Configuration
MAX_TUNING_JOBS=50
MAX_PARALLEL_JOBS=5

# Target Metrics
TARGET_ACCURACY=0.90
