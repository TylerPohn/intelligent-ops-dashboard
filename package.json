{
  "name": "iops-dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "deploy": "cd cdk && npx cdk deploy --all",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --config tests/jest.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:unit": "jest --config tests/jest.config.js tests/unit",
    "test:integration": "jest --config tests/jest.config.js tests/integration",
    "test:performance": "jest --config tests/jest.config.js tests/performance",
    "test:ci": "jest --config tests/jest.config.js --coverage --ci --maxWorkers=2",
    "c": "claude --dangerously-skip-permissions",
    "generate:insights": "./scripts/simple-generate.sh 30",
    "generate:quick": "./scripts/generate-quick.sh",
    "generate:medium": "./scripts/generate-medium.sh",
    "generate:large": "./scripts/generate-large.sh",
    "generate:test-data": "./scripts/generate-test-events.sh",
    "ml:generate": "npx ts-node scripts/ml/generate-training-data.ts",
    "ml:features": "python3 scripts/ml/feature-engineering.py",
    "ml:train": "python3 scripts/ml/train-sagemaker-model.py",
    "ml:pipeline": "npm run ml:generate && npm run ml:features && npm run ml:train",
    "ml:setup": "pip install -r scripts/ml/requirements.txt"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "aws-sdk-client-mock": "^3.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.478.0",
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/client-eventbridge": "^3.478.0",
    "@aws-sdk/client-kinesis": "^3.478.0",
    "@aws-sdk/client-s3": "^3.926.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.478.0",
    "@aws-sdk/client-sns": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "ws": "^8.18.3"
  }
}
